  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NiklasPor/nx-remotecache-custom/issues"
  },
  "homepage": "https://github.com/NiklasPor/nx-remotecache-custom#readme",
  "devDependencies": {
    "@nrwl/workspace": "^11.1.1",
    "@types/adm-zip": "^0.4.34",
    "@types/yargs": "^16.0.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "adm-zip": "^0.5.5",
    "chalk": "^4.1.0"
  },
  "peerDependencies": {
